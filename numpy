data = np.random.randint(1, 100, size=20)

mean = np.mean(data)
median = np.median(data)
std_dev = np.std(data)
maximum = np.max(data)
minimum = np.min(data)

print("Data:", data)
print("Mean:", mean)
print("Median:", median)
print("Standard Deviation:", std_dev)
print("Max:", maximum)
print("Min:", minimum)
